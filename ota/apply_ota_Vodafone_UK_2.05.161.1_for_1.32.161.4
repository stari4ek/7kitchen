source 7common

usage()
{
cat << EOF
Usage:
	`basename $0` <orig> <patch> <out>

This script apply OTA update on locally extracted ROM.
It works woth Legend_Vodafone_UK from 1.32.161.4 to 2.05.161.1 only
Android device is used for per-file patching (It doesn't overwrite anything) 
EOF
exit 1
}

DIR_ORIG=$1
DIR_PATCH=$2
DIR_PATCHED=$3
DIR_TMP=/cache/tmp

test $# -eq 3 || usage
test -d $DIR_ORIG || echo "Can't open orig dir '$DIR_ORIG'"
test -d $DIR_PATCH || echo "Can't open patch dir '$DIR_PATCH'"
test -n "$DIR_TMP" ||  echo "Output dir is missed"

rm -rf $DIR_PATCHED
cp -r $DIR_ORIG $DIR_PATCHED || 7die "failed" 1

rm "$DIR_PATCHED/system/etc/WP_0412KOR.db"
